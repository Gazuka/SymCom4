{% extends 'symcom4/admin/partials/_base_admin.html.twig' %}

{% block content %}
    <div class="card">
        <div class="card-body">
            <h1 class="h4">Création d'un nouvel utilisateur</h1>
            <p>Veuillez compléter le formulaire ci-dessous :</p>
            {{ form_start(form) }}
            {{ form_row(form.pseudo) }}
            {{ form_row(form.password) }}
            {{ text_enregistrer | BtnSubmit }}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}